FROM ubuntu
RUN echo '/bin/env  MOZ_FAKE_NO_SANDBOX=1  dbus-launch gnome-session gnome-session-properties
export XKL_XMODMAP_DISABLE=1
export XDG_CURRENT_DESKTOP="GNOME-Flashback:GNOME"
export XDG_MENU_PREFIX="gnome-flashback-"
gnome-session --session=gnome-flashback-metacity --disable-acceleration-check &'  > $HOME/.vnc/xstartup
